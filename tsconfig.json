    "declaration": true,
    "inlineSources": true,
    "sourceMap": true,
    "outDir": "./dist",
    "alwaysStrict": true,
    "strictFunctionTypes": true,
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
